{
  "presets": [
    ["@babel/preset-env", { "useBuiltIns": "usage", "corejs": { "version": "3.15", "proposals": true } }],
    "@babel/preset-react"
  ],
  "env": {
    "production": {
      "presets": ["minify"],
      "plugins": [
        [
          "transform-react-remove-prop-types",
          {
            "mode": "remove",
            "removeImport": true,
            "ignoreFilenames": ["node_modules"]
          }
        ]
      ]
    }
  }
}
